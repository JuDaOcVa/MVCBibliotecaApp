@model IEnumerable<Prj_JDOV_MVCBibliotecaApp.Models.Prestamo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaPrestamo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaDevolucion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdusuarioNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsbnNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaPrestamo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaDevolucion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdusuarioNavigation.Idusuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsbnNavigation.Isbn)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idprestamo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idprestamo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idprestamo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
